[user]
    name = Jeffrey Shih
    email = 8042780+jshih7@users.noreply.github.com
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    ls = log --oneline
    ll = ls
    st = status
    br = branch
    brr = branch --remote
    brm = branch -m
    co = checkout
    cob = checkout -b
    cp = cherry-pick
    cpa = cp --abort
    df = diff -w --color-words --color-moved
    dfc = diff --cached -w --color-words --color-moved
    dfs = dfc
    pull = pull --ff-only
    com = commit -m
    coma = commit --amend
    comr = commit --amend --reset-author
    comra = commit --amend --reset-author --date="$(git log -n 1 --format=%aD)"
    reb = rebase -i
    rebn = "!f() { \
            git rebase -i HEAD~$1; \
        }; f"
    rebc = rebase --continue
    reba = rebase --abort
    ref = reflog
[core]
    editor = /usr/bin/vim
[pager]
    branch = false
    tag = false
