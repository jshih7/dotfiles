[user]
    name = Jeffrey Shih
    email = 8042780+jshih7@users.noreply.github.com
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    co = checkout
    cp = cherry-pick
    df = diff -w --color-words --color-moved
    pull = pull --ff-only
[core]
    editor = /usr/bin/vim
